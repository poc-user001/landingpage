<mvc:View controllerName="com.golit.lp.landingpage.controller.App" 
  xmlns:mvc="sap.ui.core.mvc" 
  xmlns="sap.m" 
  xmlns:f="sap.f" 
  displayBlock="true"
  xmlns:core="sap.ui.core"
  >
  <f:ShellBar title="header" 
   homeIcon="" showMenuButton="true" showNavButton="false" showCopilot="true"
    showNotifications="false" avatarPressed="onProfileButtonPress"
    showProductSwitcher="false" menuButtonPressed="menuButtonPressed">
    <f:profile>
      <f:Avatar initials="AK"/>
    </f:profile>
    <f:searchManager>
      <f:SearchManager id="searchField" 
      suggest="handlerSearchSuggestEvent"
      enableSuggestions="true" 
      suggestionItems="{
					path: 'oModel>/SalesOrgSet',
					sorter: { path: 'salesorgname' }
				}"
        >
        <f:suggestionItems>
          <SuggestionItem description="{salesorgname}" key="{salesorg}"/>
          <SuggestionItem description="Amarjeet" key="1000"/>

        </f:suggestionItems>
      </f:SearchManager>
    </f:searchManager>
    </f:ShellBar>
  <App class="sapUiBody" id="_landingpageShellId" defaultTransitionName="flip">
  </App>
  
</mvc:View>